{
  "expo": {
    "name": "Schedura",
    "slug": "schedura",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "schedura",
    "backgroundColor": "#E9F0E9",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.shridkdev.schedura"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo.png",
        "backgroundColor": "#E9F0E9"
      },
      "package": "com.shridkdev.schedura"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo.jpg",
          "imageWidth": 200,
          "resizeMode": "cover",
          "backgroundColor": "#E9F0E9"
        }
      ],
      "expo-font",
      "expo-secure-store",
      "expo-web-browser",
      "expo-notifications"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "22517eec-3c24-45be-9497-c4365ea5aa7b"
      }
    }
  }
}
